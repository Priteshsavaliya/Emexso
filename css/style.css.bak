@import url('https://fonts.googleapis.com/css2?family=Barlow:wght@100;200;300;400;500;600;700;800;900&family=Josefin+Sans:wght@100;200;300;400;500;600;700&display=swap');


body{
	font-family: 'Barlow', sans-serif;
}

h1, h2, h3, h4, h5, h6{
	font-family: 'Josefin Sans', sans-serif;
}

.font1{
	font-family:'Barlow', sans-serif;
}

.font2{
	font-family: 'Josefin Sans', sans-serif;
}

.fs18{
	font-size: 18px;
}

.fs20{
	font-size: 20px;
}

ul, ol{
	list-style: none;
}

a{
	text-decoration: none;
}

:root{
	--orange: #ff6348;
	--white: #ffffff;
	--blue: #0c2461;
}

.header1 li i{
	color: var(--orange);
	font-size: 16px;
	padding: 7px;
}

.header1 li{
	padding-right: 20px;
}

.social li a{
	width: 35px;
	height: 35px;
	border: 1px solid var(--orange);
	border-radius: 4px;
	transition: all 0.5s;
	display: block;
	line-height: 33px;
	color: var(--orange);
}

.social li a:hover{
	background-color: var(--orange);
	color: var(--white);
}

.header2 li a{
	padding: 20px;
	color: black;
	font-weight: 500;
}

.header2 li a:hover{
	color: var(--orange);
}


.header2{
	padding: 16px 21px;
}
.header2 button{
	border: 1px solid var(--orange);
	background-color: var(--orange);
	color: white;
}

.header2 button:hover{
	background-color: transparent;
	color: var(--orange);
}

.benner w-100{
	background-color: #fbd7b5;
}